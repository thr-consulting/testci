{
  "name": "@thx/testci",
  "version": "1.0.8",
  "description": "Testing CI",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "test": "node --harmony_proxies node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "lint": "eslint *.js src/lib",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thr-consulting/testci.git"
  },
  "author": "Mike Kornelson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thr-consulting/testci/issues"
  },
  "homepage": "https://github.com/thr-consulting/testci#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.53.1",
    "identity-obj-proxy": "^3.0.0",
    "ink-docstrap": "^1.3.0",
    "jest": "^20.0.4",
    "jsdoc": "^3.5.4",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "semantic-ui-react": "^0.71.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-node-externals": "^1.6.0",
    "@storybook/react": "^3.2.5"
  },
  "dependencies": {
    "debug": "^3.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react-flow-types": "^0.1.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
